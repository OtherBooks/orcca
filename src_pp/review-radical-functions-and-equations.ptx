        $alt="This picture shows the graph of the function $f(x)=$funcNew. The points ($x,0) is labeled.";

        ($min[0], $max[0], $min[1], $max[1], $ticknum[0], $ticknum[1], $xticks_ref, $yticks_ref, $marksep[0], $marksep[1]) = NiceGraphParameters([0,$x],[-2,2]);

        @xticks = @$xticks_ref;
