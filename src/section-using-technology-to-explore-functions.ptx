                        \addplot+[domain=-8:8] {-5*x+300};
                        \addplot[soliddot, color=fourthcolor] coordinates {(-3.25,316.25)} node[above right]{$(-3.25,316.25)$};
                        \addplot[soliddot, color=fifthcolor] coordinates {(0,300)} node[below left]{$(0,300)$};
                    \end{axis}
                \end{tikzpicture}
