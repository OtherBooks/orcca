                      @xentry = ((-$b-sqrt($b**2 - 4*$a*($c-$ymin)))/(2*$a), (-$b+sqrt($b**2 - 4*$a*($c-$ymin)))/(2*$a));
                    }
                    for my $xval1 (@xentry) {
