      <exercise permid="dmZ">
        <webwork>
            <setup>

            <pg-code>
                $yes=PopUp(['?','True', 'False'],1);
                $no=PopUp(['?','True', 'False'],2);
            </pg-code>
            </setup>
            <statement>
              <p permid="AhN">
                Determine if the following statements are true or false.

                <ol permid="AoR" label="a.">
                  <li permid="oKt">
                    <p permid="Fnf">
                      <m>(a-b)^2=a^2-b^2</m>
                    </p>

                    <p permid="luo">
                      <var name="$no" form="popup"/>
                    </p>
                  </li>

                  <li permid="URC">
                    <p permid="RBx">
                      <m>(a+b)^2=a^2+b^2</m>
                    </p>

                    <p permid="xIG">
                      <var name="$no" form="popup"/>
                    </p>
                  </li>

                  <li permid="AYL">
                    <p permid="dPP">
                      <m>(a+b)(a-b)=a^2-b^2</m>
                    </p>

                    <p permid="JWY">
                      <var name="$yes" form="popup"/>
                    </p>
                  </li>
                </ol>
              </p>
            </statement>
            <solution permid="Ged">
              <p permid="koV">
                <ol permid="qVf" label="a.">
                  <li permid="hfU">
                    <p permid="qeh">
                      False, as <m>(a-b)^2=a^2-2ab+b^2</m>.
                    </p>
                  </li>

                  <li permid="Nnd">
                    <p permid="Wlq">
                      False, as <m>(a+b)^2=a^2+2ab+b^2</m>.
                    </p>
                  </li>

                  <li permid="tum">
                    <p permid="Csz">
                      True.
                    </p>
                  </li>
                </ol>
              </p>
            </solution>
        </webwork>
      </exercise>

      <exercise permid="Jui">
